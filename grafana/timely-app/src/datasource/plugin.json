{
  "type": "datasource",
  "name": "Timely",
  "id": "timely-datasource",

  "metrics": true,
  "defaultMatchFormat": "pipe",
  "annotations": true,

  "includes": [
    {"type": "dashboard", "name": "Timely Standalone Test", "path": "dashboards/standalone_test.json"},
    {"type": "dashboard", "name": "Accumulo Overview", "path": "dashboards/AccumuloOverview.json"},
    {"type": "dashboard", "name": "Hadoop Overview", "path": "dashboards/HadoopOverview.json"},
    {"type": "dashboard", "name": "System Overview", "path": "dashboards/SystemOverview.json"},
    {"type": "dashboard", "name": "Timely Status", "path": "dashboards/TimelyStatus.json"}
  ],

  "info": {
    "author": {
      "name": "National Security Agency",
      "url": "http://github.com/NationalSecurityAgency/timely"
    },
    "logos": {
      "small": "../timely-app/img/brand_small.png",
      "large": "../timely-app/img/brand_large.png"
    }
  }
}
