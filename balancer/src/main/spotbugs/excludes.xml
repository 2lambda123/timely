<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <Match>
    <Class name="timely.balancer.Balancer" />
    <Method name="shutdown" />
    <Bug pattern="UC_USELESS_OBJECT" />
  </Match>
  <Match>
    <Class name="timely.balancer.netty.http.HttpRelayHandler$MyByteArrayOutputStream" />
    <Method name="getBuf" />
    <Bug pattern="EI_EXPOSE_REP" />
  </Match>
  <Match>
    <Class name="timely.balancer.resolver.MetricAssignmentPerister" />
    <Method name="createAssignmentFile" />
    <Bug pattern="RV_RETURN_VALUE_IGNORED_BAD_PRACTICE" />
  </Match>
</FindBugsFilter>
